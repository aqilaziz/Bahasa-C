#include <stdio.h>

main(){
	int panjang, lebar, luas;
	
	printf ("PROGRAM MENGHITUNG LUAS PERSEGI\n\n");
	
	
	printf ("Masukkan ukuran panjang \t: ");
	scanf ("%d",&panjang);
	
	printf ("Masukkan ukuran lebar \t\t: ");
	scanf ("%d",&lebar);
	
	luas = panjang * lebar;
	
	printf ("\n\nJadi luas persegi tersebut adalah %d meter persegi",luas);
	
	return 0;
	
}
